@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv=Content-Type content="text/html;charset=utf-8">
    <meta charset="gbk">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta content="black" name="apple-mobile-web-app-status-bar-style">
    <meta content="telephone=no" name="format-detection" />
    <meta content="yes" name="apple-touch-fullscreen" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,minimum-scale=1.0,user-scalable=no" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <title>新增会议室预订</title>
    <link href="~/DDStatic/css/reset.css" rel="stylesheet" />
    <link href="~/DDStatic/css/task.css" rel="stylesheet" />
    <script>
        var _config = @Html.Raw(FoxOne.Core.JSONHelper.Serialize(FoxOne.Business.OAuth.DDHelper.FetchSignPackage(HttpContext.Current.Request)))   ;
        var meetingRoom=@Html.Raw(ViewData["MeetingRoom"]) ;
    </script>

</head>
<body>
    <div class="doc">
        <div class="task-add" id="task-add">
            <div class="cells cells-form">
                <div class="cell">
                    <div class="cell-hd"><label class="label">会议主题</label></div>
                    <div class="cell-bd cell-primary">
                        <input class="input" type="text" id="Title" name="Title" placeholder="请输入">
                    </div>
                </div>
                <div class="cell">
                    <div class="cell-hd"><label class="label">开始日期</label></div>
                    <div class="cell-bd cell-primary datepicker">
                        <div class="date" id="StartDate">@DateTime.Now.ToString("yyyy-MM-dd")</div>
                    </div>
                </div>
                <div class="cell">
                    <div class="cell-hd"><label class="label">开始时间</label></div>
                    <div class="cell-bd cell-primary timepicker">
                        <div class="date" id="StartTime">@DateTime.Now.ToString("HH:mm")</div>
                    </div>
                </div>
                <div class="cell">
                    <div class="cell-hd"><label class="label">结束日期</label></div>
                    <div class="cell-bd cell-primary datepicker">
                        <div class="date" id="EndDate">@DateTime.Now.ToString("yyyy-MM-dd")</div>
                    </div>
                </div>
                <div class="cell">
                    <div class="cell-hd"><label class="label">结束时间</label></div>
                    <div class="cell-bd cell-primary timepicker">
                        <div class="date" id="EndTime">@DateTime.Now.AddHours(1).ToString("HH:mm")</div>
                    </div>
                </div>
                <div class="cell">
                    <div class="cell-hd"><label class="label">会议室</label></div>
                    <div class="cell-bd cell-primary select-type">
                        <div class="select"></div>
                        <input type="hidden" name="MeetingRoomId" id="MeetingRoomId" />
                    </div>
                </div>
                <div class="cell">
                    <div class="cell-hd"><label class="label">与会人员</label></div>
                    <div class="cell-bd cell-primary">
                        <input class="input" type="text" id="Description" name="Description" placeholder="选择与会人员">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Scripts/zepto.min.js"></script>
    <script type="text/javascript" src="http://g.alicdn.com/dingding/open-develop/1.6.9/dingtalk.js"></script>
    <script src="~/DDStatic/js/fastclick.js"></script>
    <script src="~/DDStatic/js/add.js"></script>
</body>
</html>
